install-nobase_pddataDATA: $(nobase_pddata_DATA)
	@$(NORMAL_INSTALL)
	@list='$(nobase_pddata_DATA)'; test -n "$(pddatadir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(pddatadir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(pddatadir)" || exit 1; \
	fi; \
	$(am__nobase_list) | while read dir files; do \
	  xfiles=; for file in $$files; do \
	    if test -f "$$file"; then xfiles="$$xfiles $$file"; \
	    else xfiles="$$xfiles $(srcdir)/$$file"; fi; done; \
	  test -z "$$xfiles" || { \
	    test "x$$dir" = x. || { \
	      echo " $(MKDIR_P) '$(DESTDIR)$(pddatadir)/$$dir'"; \
	      $(MKDIR_P) "$(DESTDIR)$(pddatadir)/$$dir"; }; \
	    echo " $(INSTALL_DATA) $$xfiles '$(DESTDIR)$(pddatadir)/$$dir'"; \
	    $(INSTALL_DATA) $$xfiles "$(DESTDIR)$(pddatadir)/$$dir" || exit $$?; }; \
	done

uninstall-nobase_pddataDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(nobase_pddata_DATA)'; test -n "$(pddatadir)" || list=; \
	$(am__nobase_strip_setup); files=`$(am__nobase_strip)`; \
	dir='$(DESTDIR)$(pddatadir)'; $(am__uninstall_files_from_dir)
